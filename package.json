{
  "name": "peepohappy",
  "version": "2.0.0",
  "description": "Try Ajitiono's personal blog",
  "repository": "git@github.com:imballinst/peepohappy.git",
  "author": "Try Ajitiono <ballinst@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "prebuild": "node scripts/activity.js",
    "build": "PUBLIC_ASTRO_ENV=production astro build && yarn go:build",
    "go:build": "cd netlify/functions/comments-go && GOOS=linux GOARCH=amd64 go build comments.go",
    "postbuild": "node scripts/optimize-image/sharp.js && yarn go:postbuild",
    "go:postbuild": "mv netlify/functions/comments-go/comments netlify/functions/comments",
    "preview": "astro preview"
  },
  "devDependencies": {
    "@octokit/rest": "18.12.0",
    "astro": "0.21.13",
    "autoprefixer": "10.4.0",
    "cheerio": "1.0.0-rc.10",
    "fs-extra": "10.0.0",
    "netlify-cli": "9.4.0",
    "prettier": "2.4.1",
    "prettier-plugin-astro": "0.0.11",
    "remark": "14.0.2",
    "sharp": "0.29.3",
    "tailwindcss": "3.0.7",
    "typescript": "4.5.4",
    "typescript-plugin-css-modules": "3.4.0",
    "vfile-reporter": "7.0.3",
    "yaml": "1.10.2"
  },
  "peerDependencies": {
    "preact": "^10.6.4"
  },
  "dependencies": {
    "@tailwindcss/forms": "0.4.0",
    "date-fns": "2.27.0"
  }
}
